{
  "name": "amcrest2mqtt",
  "version": "0.04",
  "slug": "amcrest2mqtt",
  "description": "Amcrest Dorbell add-on!",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "options": {
    "AMCREST_HOST": "192.168.0.232",
    "AMCREST_PORT": 80,
    "AMCREST_USERNAME": "admin",
    "AMCREST_PASSWORD": "admin",
    "MQTT_USERNAME": "mosquitto",
    "MQTT_PASSWORD": " ",
    "MQTT_HOST": "localhost",
    "MQTT_QOS": 0,
    "MQTT_PORT": 1883,
    "HOME_ASSISTANT": false,
    "HOME_ASSISTANT_PREFIX": "homeassistant"

  },
  "schema": {
    "AMCREST_HOST": "str",
    "AMCREST_PORT": "int",
    "AMCREST_USERNAME": "str",
    "AMCREST_PASSWORD": "str",
    "MQTT_USERNAME": "str",
    "MQTT_PASSWORD": "str",
    "MQTT_HOST": "str",
    "MQTT_QOS": "int",
    "MQTT_PORT": "int",
    "HOME_ASSISTANT": "bool",
    "HOME_ASSISTANT_PREFIX": "str"
  }
